<h1 class="text-center">City Page</h1>
<h3>Map Overview</h3>
<div id="map"></div>
<hr>
<% for(var i = 0; i < city.iziTitle.length; i++) { %>
  <div>
    <h2><%= city.iziType[i].charAt(0).toUpperCase() + city.iziType[i].slice(1) %>: <%= city.iziTitle[i] %></h2>
    <hr/>
    <h3><%= city.iziCity[i] %></h3>
    <h4>lat, lon: <%= city.iziLatitude[i] %>, <%= city.iziLongitude[i] %> <%= city.iziCountry[i] %></h4>
    <img src=<%= city.imageFile[i] %>>
    <%= city.iziDescription %>
    <h2>Original Object: <%= city.iziOriginal[i] %></h2>
  </div>
  <hr/>
<% }; %>

<script>
  console.log("<%= city.iziCountry[0] %>");
  function initMap() {
    markers = [];
    var center = {lat: <%= city.iziLatitude[0] %>, lng: <%= city.iziLongitude[0] %>};
    var center2 = {lat: <%= city.iziLatitude[1] %>, lng: <%= city.iziLongitude[1] %>};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 12,
      center: center
    });
    var marker = new google.maps.Marker({
      position: center,
      map: map
    });
    var marker2 = new google.maps.Marker({
      position: center2,
      map: map
    });
    // var marker3 = new google.maps.Marker({
    //   position: center3,
    //   map: map
    // });
    console.log("<%= city.iziCountry[0] %>");
  }
</script>
<script async defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDJq43DKyz36PwuZu9EOMLvbocgHn8FTsM&callback=initMap">
</script>